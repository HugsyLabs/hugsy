{
  "name": "@hugsylabs/hugsy-ui",
  "version": "0.2.0",
  "description": "Modern UI for Hugsy configuration management",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"node server.js\"",
    "server": "node server.js",
    "dev:frontend": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "keywords": [
    "hugsy",
    "ui",
    "configuration",
    "claude"
  ],
  "author": "Hugsy Contributors",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@hugsylabs/hugsy-core": "workspace:^0.2.0",
    "@monaco-editor/react": "^4.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "framer-motion": "^12.23.12",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-resizable-panels": "^3.0.5",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3",
    "vite": "^5.4.19"
